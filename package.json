{
  "name": "elections",
  "version": "1.0.0",
  "description": "Web application that helps find poll locations and elected officials base on address.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/CityOfDetroit/electionsgit"
  },
  "keywords": [
    "sms",
    "elections",
    "tool",
    "geospatial",
    "poll locations",
    "voting",
    "mapbox",
    "alerts",
    "js",
    "react",
    "jest"
  ],
  "bugs": {
    "url": "https://github.com/CityOfDetroit/elections/issues"
  },
  "homepage": "https://github.com/CityOfDetroit/elections#readme",
  "scripts": {
    "start": "parcel src/index.html --no-cache -p 3000",
